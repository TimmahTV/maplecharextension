{
  "name": "maplecharextension",
  "version": "0.0.1",
  "description": "A browser extension that stores maplestory characters and fills their relevant data into existing tools",
  "scripts": {
    "build": "node ./build_scripts/clearBuildFolder.js && npx webpack && node ./build_scripts/build.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generateLocaleFile": "node ./build_scripts/generateLocaleFile.js",
    "format": "npx prettier --write \"./src/**/*.{js,jsx,ts,tsx}\""
  },
  "author": "timmah",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^5.0.3",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "@types/chrome": "^0.0.250",
    "fs-extra": "^11.1.1",
    "prettier": "^3.0.3",
    "webpack": "^5.77.0"
  },
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
