{
  "manifest_version": 3,
  "name": "Maplestory Character Extension",
  "default_locale": "en",
  "description": "Create Maplestory characters, use them for analysis, and use preexisting webtools as well.",
  "version": "1.0",
  "icons": {
    "16": "images/snail-16.png",
    "32": "images/snail-32.png",
    "48": "images/snail-48.png",
    "128": "images/snail-128.png"
  },
  "background": {
    "service_worker": "background/bg.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/snail-16.png",
      "32": "images/snail-32.png",
      "48": "images/snail-48.png",
      "128": "images/snail-128.png"
    }
  },
  "permissions": ["scripting", "activeTab"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+M",
        "mac": "Command+Shift+M"
      }
    }
  },
  "short_name":"MapleChar"
}
